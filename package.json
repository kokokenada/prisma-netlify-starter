{
  "name": "typescript-graphql",
  "scripts": {
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run db-deploy-gen && npm run graphql-gen",
    "db-deploy-gen": "./db-deploy-gen.sh",
    "graphql-gen": "./graphql-gen.sh",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly src/server",
    "seed": "ts-node prisma/seed"
  },
  "dependencies": {
    "@prisma/photon": "2.0.0-preview019",
    "@types/node-fetch": "2.5.4",
    "apollo-server": "2.25.4",
    "apollo-server-lambda": "^2.9.14",
    "graphql": "14.5.8",
    "graphql-middleware": "^4.0.2",
    "nexus": "0.12.0-rc.5",
    "nexus-prisma": "0.6.1"
  },
  "devDependencies": {
    "@prisma/sdk": "0.0.103",
    "@types/node": "12.12.21",
    "prettier": "^1.19.1",
    "prisma2": "2.0.0-preview019",
    "ts-node": "8.5.4",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.7.3"
  },
  "prettier": {
    "semi": true,
    "tabWidth": 2,
    "printWidth": 140,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
